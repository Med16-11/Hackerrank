oid insertionSort1(int n, vector<int> arr)
{ 
    int val=arr[n-1],flag=0;
        
        for(int i=n-1;i>=0;i--)
        {
            if(val<arr[i-1])
            {
                arr[i]=arr[i-1];
            }
            else
            {
                arr[i]=val;
                flag=1;
            }
            for(int i = 0; i < n; i++) {
                cout<<arr[i]<<" ";
                }
            cout<<"\n";
            if(flag==1)
            break;
        }
}
